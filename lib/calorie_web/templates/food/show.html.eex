<h1>Show Food</h1>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @food.name %>
  </li>

  <li>
    <strong>Calories:</strong>
    <%= @food.calories %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.food_path(@conn, :edit, @food) %></span>
<span><%= link "Back", to: Routes.food_path(@conn, :index) %></span>
